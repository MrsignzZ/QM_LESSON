<!--miniprogram/pages/photo/photo.wxml-->
<image class="headImage" src="{{photo}}" mode="aspectFill"></image>
<view style="padding:0px 50px">
  <van-button class="dBtn" bindTap="downloadPhoto">下载到本地</van-button>
</view>
<view style="padding:0px 50px">
  <van-button class="dBtn" bindTap="sharePhoto">分享到朋友圈</van-button>
</view>
<view style="padding:0px 50px">
  <van-button class="dBtn">生成临时链接</van-button>
</view>
