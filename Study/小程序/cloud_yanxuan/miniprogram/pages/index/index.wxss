.adInfos{
  width:100%;
  height: 400rpx;
  position: relative;
}
.adInfos swiper{
  height: 100%;
}
.adInfos swiper image{
  position: absolute;
  width:100%;
  height: 100%;
}

.servicePolicy {
  display: inline-block;
  width: 100%;
}
.servicePolicy-grow {
  width: 750rpx;
  box-sizing: border-box;
  margin: 0 auto;
  text-align: center;
}
.text {
  font-size: 28rpx;
  color: #333;
  margin-left: 0.10667rem;
  line-height: 18px;
  display: inline-block;
}
.icon {
  display: inline-block;
  background-repeat: no-repeat;
  width: 16px;
  height: 16px;
}

.welfare_img {
  display: block;
  width: 750rpx;
  height: 160rpx;
}

.img {
  width: 100%;
  height: 100%;
}

.content_h1 {
  width: 100%;
  height: 100rpx;
  position: relative;
}
.content-title {
  position: absolute;
  top: 0;
  bottom: 0;
  margin: auto;
  display: inline-block;
  width: 100%;
  height: 50rpx;
  padding:0 40rpx;
  box-sizing: border-box;
}

.content_h1_title-txt {
  display: block;
  float: left;
  font-size: 35rpx;
  line-height: 40rpx;
  letter-spacing: 2px;
  font-weight: bold;
  font-family: PingFangSC-Regular;
}

.txt1 {
  color: #424242;
}

.txt2 {
  color: #B9353A;
}

.content_h1_title-more {
  float: right;
}

