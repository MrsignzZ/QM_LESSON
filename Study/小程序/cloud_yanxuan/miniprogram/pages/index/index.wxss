.search {
  width: 100%;
}
.van-search .field-index--van-field {
  background: #fff;
  border:solid 0.5px;
  border-radius: 28rpx;
  width: 100%;
}
.adInfos{
  width:100%;
  height: 400rpx;
  position: relative;
}
.adInfos swiper{
  width: 100%;
  height: 100%;
  margin: 5rpx auto;
}
.adInfos swiper swiper-item {
  padding: 0 5px;
  box-sizing: border-box;
}
.adInfos swiper image{
  position: absolute;
  width: 100%;
  height: 100%;
  border-radius: 15px;
}

.servicePolicy {
  display: inline-block;
  width: 100%;
}
.servicePolicy-grow {
  width: 750rpx;
  box-sizing: border-box;
  margin: 0 auto;
  text-align: center;
}
.text {
  font-size: 28rpx;
  color: #333;
  margin-left: 0.10667rem;
  line-height: 18px;
  display: inline-block;
}
.icon {
  display: inline-block;
  background-repeat: no-repeat;
  width: 16px;
  height: 16px;
}

.welfare_img {
  display: block;
  width: 750rpx;
  height: 160rpx;
}

.img {
  width: 100%;
  height: 100%;
}

.content_h1 {
  width: 100%;
  height: 100rpx;
}
.content-title {
  margin-top: 10rpx;
  margin-bottom: 10rpx;
  display: inline-block;
  width: 100%;
  height: 80rpx;
  padding:0 40rpx;
  box-sizing: border-box;
}

.content_h1_title-txt {
  display: block;
  float: left;
  font-size: 35rpx;
  line-height: 40rpx;
  letter-spacing: 2px;
  font-weight: bold;
  font-family: PingFangSC-Regular;
}

.txt1 {
  color: #424242;
}

.txt2 {
  color: #B9353A;
}

.content_h1_title-more {
  float: right;
}

.collage{
  width:100%;
  height: 200rpx;
  text-align: center;
  background-color: #F8F8F4;
}
.collage swiper{
  height: 100%;
}
.collage swiper image{
  /* position: absolute; */
  width: 125rpx;
  height: 150rpx;
  /* height: 200rpx; */
  border-radius: 15px;
}
.desc {
  font-size: 20rpx;
  color: #B63034;
}

